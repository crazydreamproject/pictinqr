{
  "expo": {
    "name": "pictonqr",
    "slug": "pictonqr",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cdp.pictonqr",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan qrcodes."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "versionCode": 2,
      "permissions": [
        "CAMERA"
      ],
      "package": "com.cdp.pictonqr"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": ""
  }
}
